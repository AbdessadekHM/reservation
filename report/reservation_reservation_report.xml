<odoo>

    <record id="reservation_report_search_view" model="ir.ui.view">
        <field name="name">Reservation Report Search</field>
        <field name="model">reservation.reservation.report</field>
        <field name="arch" type="xml">
            <search>
                <field name="partner_id"  />
                <!-- <field name="state"  /> -->

                <filter string="Partner" name="group_by_partner" context="{'group_by': 'partner_id'}"/>
                <filter string="State" name="group_by_state" context="{'group_by': 'state'}"/>
            </search>


        </field>

    </record>

    <record id="reservation_report_window_action" model="ir.actions.act_window">
        <field name="name">Reservation Report</field>
        <field name="res_model">reservation.reservation.report</field>
        <field name="view_mode">list,pivot,graph</field>
        <field name="context">
            {
                'search_default_group_by_partner': 1,
                'search_default_group_by_state': 2

            }
            
        </field>

        <!-- <field name="context">{'fill_temporal': 0, 'search_default_group_by_partner_id': 1, 'search_default_group_by_state': 2}</field> -->

    </record>

</odoo>